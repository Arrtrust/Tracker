{
    "name": "arrtrust/tracker",
    "description": "Simple site visit/statistics tracker for Laravel 5",
    "license": "MIT",
    "keywords": [
        "tracker",
        "analytics",
        "visitor",
        "statistics",
        "laravel",
        "laravel5"
    ],
    "version": "1.6.3",
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Arrtrust\\Tracker\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    },
    "suggest": {
        "laravel/framework": "This package requires Laravel 5."
    }
}
